{
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "object-curly-spacing": ["error", "always"],
    "arrow-parens": ["error", "as-needed"]
  },
  "extends": ["standard", "standard-react"],
  "globals": {
    "it": true,
    "window": true,
    "Headers": true,
    "fetch": true,
    "URL": true
  },
  "env": {
    "browser": true,
    "jest": true
  }
}